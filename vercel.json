{
  "version": 2,
  "env": {
    "MONGO_URI": "MONGO_URI",
    "SERVER_PORT": "SERVER_PORT",
    "JWT_SECRET": "JWT_SECRET"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}
